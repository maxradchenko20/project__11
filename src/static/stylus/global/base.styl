body
    padding 0
    margin 0
.container
    max-width 1100px
    margin 0 auto
    width 90%
a  
    text-decoration none
li
    list-style-type none
ul 
    list-style none 
img
    max-width 100%
.btn
    width 100px
    height: 40px
    background-color: white
    border 1px solid #D3D3D3
    border-radius 5px
    text-transform uppercase
    color #D3D3D3
    cursor pointer
.btn:hover
    color #000000
    border 2px solid #000000
    transition .3s
    margin-left: 2px

